{
  // Gitlens config
  "gitlens.keymap": "none",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": true,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  // Vim Config
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.enableNeovim": true,
  "vim.easymotion": true,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.visualstar": true,
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "j"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "k",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    },
    {
      "before": [
        "h",
        "h"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "g",
        "c"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentLine",
          "args": []
        }
      ]
    },
    {
      "before": [
        "g",
        "C"
      ],
      "after": [],
      "commands": [
        {
          "command": "editor.action.commentBlock",
          "args": []
        }
      ]
    },
    {
      "before": [
        "s",
        "d"
      ],
      "after": [
        "leader",
        "leader",
        "s",
        "<char>"
      ]
    },
    {
      "before": [
        "s",
        "s"
      ],
      "after": [
        "leader",
        "leader",
        "2",
        "s",
        "<char>",
        "<char>"
      ]
    },
    {
      "before": [
        "s",
        "f"
      ],
      "after": [
        "leader",
        "leader",
        "f",
        "<char>"
      ]
    },
    {
      "before": [
        "s",
        "b"
      ],
      "after": [
        "leader",
        "leader",
        "F",
        "<char>"
      ]
    },
    {
      "before": [
        "s",
        "j"
      ],
      "after": [
        "leader",
        "leader",
        "j",
      ]
    },
    {
      "before": [
        "s",
        "k"
      ],
      "after": [
        "leader",
        "leader",
        "k",
      ]
    },
    {
      "before": [
        "s",
        "l"
      ],
      "after": [
        "leader",
        "leader",
        "l"
      ]
    },
  ],
  "vim.handleKeys": {
    "<C-h>": false
  },
  "vim.leader": ";",
  "vim.easymotionMarkerForegroundColorOneChar": "#ff0000",
  // Vim - Surround
  "vim.surround": true,
  // Source-Kit
  "sourcekit-lsp.toolchainPath": "/Library/Developer/Toolchains/swift-latest.xctoolchain",
  // VSCode Config
  // Editor setting
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": true,
  "editor.renderWhitespace": "none",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "editor.acceptSuggestionOnEnter": "on",
  "editor.quickSuggestionsDelay": 10,
  "editor.suggestOnTriggerCharacters": true,
  "editor.wordBasedSuggestions": true,
  "editor.wordWrap": "on",
  "editor.renderLineHighlight": "line",
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "workbench.editor.enablePreview": false,
  // Window setting
  "window.title": "${activeEditorLong}",
  "window.zoomLevel": 0,
  // File setting
  "files.autoSave": "afterDelay",
  "files.associations": {
    "*.vue": "vue"
  },
  // Code Runner
  "code-runner.runInTerminal": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.executorMapByFileExtension": {
    ".go": "go",
    ".swift": "swift",
    ".m": "Objc"
  },
  "code-runner.executorMap": {
    "javascript": "node",
    "php": "C:\\php\\php.exe",
    "python": "python",
    "perl": "perl",
    "ruby": "C:\\Ruby23-x64\\bin\\ruby.exe",
    "go": "go run",
    "html": "\"C:\\Program Files (x86)\\Google\\Chrome\\Application\\chrome.exe\"",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },
  // File path
  "path-intellisense.absolutePathToWorkspace": false,
  // Comment
  "better-comments.multilineComments": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontSize": 14,
  "vim.easymotionMarkerFontSize": "18",
  "vim.easymotionMarkerHeight": 16,
  "sourcekit-lsp.serverPath": "/usr/local/bin/sourcekit-lsp",
  "vim.highlightedyank.enable": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "C_Cpp.updateChannel": "Insiders",
  "editor.largeFileOptimizations": false,
  "breadcrumbs.enabled": true,
  "editor.renderControlCharacters": false,
  "vim.camelCaseMotion.enable": true,
  // SDE
  "sde.languageServerMode": "sourcekit-lsp",
  "swift.targets": [
    {
      "name": "NetworkMonitor",
      "path": "NetworkMonitor/NetworkMonitor",
      "sources": [
        "**/*.swift"
      ],
      "compilerArguments": [
        "-sdk",
        "/Applications/Xcode.app/Contents/Developer/Platforms/iPhoneOS.platform/Developer/SDKs/iPhoneOS.sdk",
        "-target",
        "arm64-apple-ios12.0"
      ]
    },
  ],
  "swift.path.shell": "/bin/zsh",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "workbench.editor.limit.perEditorGroup": true,
  "cSpell.userWords": [
    "relativenumber"
  ],
  "vim.statusBarColorControl": true,
  "vim.vimrc.enable": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "vim.easymotionMarkerForegroundColorTwoChar": "#fdb32b",
  "vim.easymotionKeys": "fjdksweoavn",
  "vim.easymotionMarkerWidthPerChar": 10,
  "vim.easymotionMarkerBackgroundColor": "#007accd0",
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "tabnine.experimentalAutoImports": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.pythonPath": "/usr/local/opt/python@3.8/bin/python3.8",
  "java.semanticHighlighting.enabled": true,
  "python.languageServer": "Microsoft",
  "dart.checkForSdkUpdates": false,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "editor.accessibilitySupport": "off"
}
